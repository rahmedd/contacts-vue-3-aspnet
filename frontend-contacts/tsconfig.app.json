{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
		"composite": true,
		"baseUrl": ".",
		"paths": {
		"@/*": ["./src/*"]
		}
	},
	"vueCompilerOptions": {
		"plugins": [
			// "@volar/vue-language-plugin-pug"
			"@vue/language-plugin-pug"
		]
	}
}
